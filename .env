# Default development environment for navy_encrypt_mobile
# ค่านี้ปลอดภัย สามารถแก้บนเครื่อง dev ได้ตามต้องการ
APP_ENV=development
NAVY_API_BASE_URL=https://navenc.navy.mi.th/navy-api
RELEASE_CHANNEL=internal

# Android signing (debug ใช้ค่า default ของ Flutter ได้เลย)
ANDROID_KEYSTORE_PATH=
ANDROID_KEYSTORE_PASSWORD=
ANDROID_KEY_ALIAS=
ANDROID_KEY_PASSWORD=

# iOS signing (debug --no-codesign ไม่จำเป็นต้องตั้งค่า)
IOS_BUNDLE_IDENTIFIER=th.mi.navy.navy-encrypt
IOS_TEAM_ID=

# Windows packaging
WINDOWS_PUBLISHER_ID=
