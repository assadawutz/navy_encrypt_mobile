# Environment configuration for navy_encrypt_mobile
# Copy this file to `.env` and fill in production credentials before building.

APP_ENV=production
NAVY_API_BASE_URL=https://navenc.navy.mi.th/navy-api
RELEASE_CHANNEL=production

# Android signing
ANDROID_KEYSTORE_PATH=
ANDROID_KEYSTORE_PASSWORD=
ANDROID_KEY_ALIAS=
ANDROID_KEY_PASSWORD=

# iOS signing
IOS_BUNDLE_IDENTIFIER=th.mi.navy.navy-encrypt
IOS_TEAM_ID=

# Windows packaging
WINDOWS_PUBLISHER_ID=
